{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/Frontend/$1" }
  ],
  "buildCommand": "cd Frontend && rm -rf node_modules && rm -f package-lock.json && npm install && npm run build",
  "outputDirectory": "Frontend/.next",
  "installCommand": "cd Frontend && npm install"
}